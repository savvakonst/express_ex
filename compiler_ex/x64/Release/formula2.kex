

function calc(i, j,num)
  v = i-j**i+[num,num+1,num+2]
  return v
end


function calc_new(i, j)
  a = 0*1
  v = i+j**i+cos(1)
  return a
end


function sgn(arg )
  return arg ? -1 : 1
end

