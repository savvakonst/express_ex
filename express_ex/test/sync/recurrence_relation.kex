

x = param("#1_A01__2")




function rms(arg):
    function recurrence_relation( a , cond):
       increment = ( a[0] * a[0] ) # if ( a[0] * a[0] ) replace with ( a* a) issue occur
       c = recurrence_relation[-1] + increment
       res = cond ? increment : c
       return double( res )
    end


    function get_num( a , cond ):
       c = get_num[-1] + 0 * a + 1
       res = cond ? 1 : c
       return double( res )
    end


    cond = (arg[ 0] < 0) * (arg[-1] > 0)


    num = get_num(arg, cond)
    intermediate = recurrence_relation(arg, cond)

    res = cond ? (intermediate[-1] / num[-1] )** 0.5 : rms[-1]

    return double(res)
end






return rms(x*1)