

x = param("#1_A01__2")




function rms(arg):
    function recurrence_relation( a):
       cond = (a[ 0] < 0) * (a[-1] > 0)
       c = recurrence_relation[-1] + a * a
       res = cond ? 0 : c
       return double(res )
    end




    function get_num( ar ):
       cond = (ar[ 0] < 0) * (ar[-1] > 0)
       c = get_num[-1] + 0 * ar + 1
       res = cond ? 1 : c
       return double( res )
    end

    cond = (arg[ 0] < 0) * (arg[-1] > 0)
    num = get_num(arg*1)


    intermediate = recurrence_relation( arg *1  )**0.5

    res = cond ? intermediate[-100] *num[-100] : rms[-1]

    return double(res)
end






return rms(x*1)